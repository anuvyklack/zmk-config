/* Symbols */

/* RU: !"№;%:?*()_ */
/* US: !@#$%^&*()_ */

ZMK_UNICODE_SINGLE(dot,        N0, N0, N2, E)  // .
ZMK_UNICODE_SINGLE(comma,      N0, N0, N2, C)  // ,
ZMK_UNICODE_SINGLE(colon,      N0, N0, N3, A)  // :
ZMK_UNICODE_SINGLE(semicolon,  N0, N0, N3, B)  // ;

ZMK_UNICODE_SINGLE(caret,      N0, N0, N5, E)  // ^
ZMK_UNICODE_SINGLE(dollar,     N0, N0, N2, N4) // $

ZMK_UNICODE_SINGLE(tilde,      N0, N0, N7, E)  // ~
ZMK_UNICODE_SINGLE(grave,      N0, N0, N6, N0) // `
ZMK_UNICODE_SINGLE(dquotes,    N0, N0, N2, N2) // "
ZMK_UNICODE_SINGLE(hash,       N0, N0, N2, N3) // #
ZMK_UNICODE_SINGLE(at,         N0, N0, N4, N0) // @
ZMK_UNICODE_SINGLE(ampersand,  N0, N0, N2, N6) // &

ZMK_UNICODE_SINGLE(bslash,     N0, N0, N5, C)  // \ backslash
ZMK_UNICODE_SINGLE(pipe,       N0, N0, N7, C)  // |
ZMK_UNICODE_SINGLE(slash,      N0, N0, N2, F)  // /
ZMK_UNICODE_SINGLE(question,   N0, N0, N3, F)  // ?

ZMK_UNICODE_SINGLE(lbrace,     N0, N0, N7, B)  // {
ZMK_UNICODE_SINGLE(rbrace,     N0, N0, N7, D)  // }
ZMK_UNICODE_SINGLE(lbracket,   N0, N0, N5, B)  // [
ZMK_UNICODE_SINGLE(rbracket,   N0, N0, N5, D)  // ]
ZMK_UNICODE_SINGLE(less,       N0, N0, N3, C)  // <
ZMK_UNICODE_SINGLE(greater,    N0, N0, N3, E)  // >

ZMK_UNICODE_SINGLE(unders,     N0, N0, N5, F)  // _ underscore
ZMK_UNICODE_SINGLE(nbsp,       N0, N0, A, N0)  //   no-break space

ZMK_UNICODE_SINGLE(laquo,      N0, N0, A,  B)  // «
ZMK_UNICODE_SINGLE(raquo,      N0, N0, B,  B)  // »

ZMK_UNICODE_SINGLE(en_dash,    N2, N0, N1, N3) // –
ZMK_UNICODE_SINGLE(em_dash,    N2, N0, N1, N4) // —
ZMK_UNICODE_SINGLE(h_bar,      N2, N0, N1, N5) // ―

ZMK_UNICODE_SINGLE(bullet,     N2, N0, N2, N2) // •
ZMK_UNICODE_SINGLE(h_ellipsis, N2, N0, N2, N6) // …

